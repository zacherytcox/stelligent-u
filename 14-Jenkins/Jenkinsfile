pipeline {

    agent any
    environment { 
        lab = '14'
    }
    stages{
        stage("One"){
            steps{

                sh 'ls -alh'
                sh 'pwd'
                sh 'whoami'
                // git clone https://github.com/zacherytcox/stelligent-u.git         

            }
        }

        stage("Two"){
            tools {
                maven 'apache-maven-3.0.1' 
            }
            steps{
                
                sh 'ls -alh'
                sh 'pwd'
                sh 'whoami'
                sh 'cd my-app'
                sh 'mvn clean validate compile test package' //` of the root Java project source "`my-app`"
                
            }
        }
    }

}